{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@now/node"
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "index.js"
      },
      {
        "source": "/items/(.*)",
        "destination": "/routes/items.js"
      }
    ]
  }